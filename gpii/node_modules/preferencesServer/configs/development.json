{
    "typeName": "preferencesServer.development",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "distributeOptions": [{
            "source": "{that}.options.devUrls",
            "target": "{that preferencesServer}.options.urls"
        }, {
            "source": "{that}.options.basicAuth",
            "target": "{that preferencesServer}.options.auth"
        }],
        "devUrls": {
            "rawPreferences": "http://localhost:5984/rawPreferences/%userToken"
        },
        "basicAuth": "RAW_PREFERENCES_HTTP_USERNAME:PASSWORD",
        "components": {
            "server": {
                "type": "kettle.server",
                "options": {
                    "logging": true,
                    "components": {
                        "preferencesServer": {
                            "type": "gpii.preferencesServer",
                            "options": {
                                "gradeNames": ["gpii.preferencesServer.dev"]
                            }
                        }
                    }
                }
            }
        }
    },
    "includes": [
        "./base.json"
    ]
}